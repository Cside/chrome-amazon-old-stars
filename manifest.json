{
  "version": "0.0.1",
  "name": "Amazonの星を元に戻す",
  "description": "Amazonの評価の星のデザインを以前のものに戻す",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.amazon.co.jp/*"],
      "js": ["src/contentScript.tsx"],
      "run_at": "document_end"
    }
  ]
}
